{
    "brand_identity": {
        "concept": "Luxury x Sustainability (Quiet Luxury)",
        "target_audience": "High-end eco-conscious travelers"
    },
    "color_palette": {
        "primary": {
            "forest_green": "#2D423F",
            "description": "Base color for depth and natural luxury"
        },
        "secondary": {
            "sand_beige": "#D2B48C",
            "description": "Background texture and warmth"
        },
        "accent": {
            "terracotta": "#E2725B",
            "description": "Call to action and focus highlights"
        },
        "neutral": {
            "white": "#FFFFFF",
            "off_white": "#F5F5DC",
            "deep_green_footer": "#1B2E25"
        }
    },
    "typography": {
        "headings": {
            "font_family": "Cormorant Garamond",
            "style": "Serif",
            "usage": "Elegant, organic headlines"
        },
        "body": {
            "font_family": "Inter",
            "style": "Sans-serif",
            "usage": "High readability for descriptions and data"
        }
    },
    "ui_components": {
        "navigation": {
            "type": "Sticky",
            "effect": "Glassmorphism",
            "css": {
                "backdrop-filter": "blur(20px)",
                "background": "rgba(255, 255, 255, 0.15)",
                "border-bottom": "1px solid rgba(255, 255, 255, 0.2)"
            }
        },
        "cards": {
            "layout": "Bento Grid",
            "border_radius": "16px",
            "gap": "32px"
        },
        "forms": {
            "style": "Minimalist",
            "effect": "Glassmorphism (Blur 40px)",
            "input_style": "Underline only"
        }
    }
}